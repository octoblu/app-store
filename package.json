  "license": "SEE LICENSE IN LICENSE.TXT",
  "bugs": {
    "url": "https://github.com/octoblu/app-store/issues"
  },
  "homepage": "https://github.com/octoblu/app-store#readme",
  "nyc": {
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "extension": [
      ".coffee"
    ]
  }
