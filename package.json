    "classnames": "^2.2.5",
    "css-loader": "^0.22.0",
    "css-modules-require-hook": "^4.0.6",
    "file-loader": "^0.11.1",
    "filesize": "^3.5.6",
    "gzip-size": "^3.0.0",
    "html-loader": "^0.4.5",
    "html-webpack-inline-source-plugin": "^0.0.8",
    "html-webpack-plugin": "^2.24.0",
    "lodash": "^4.0.0",
    "postcss-loader": "^2.0.5",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-icons": "^1.0.3",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "react-social": "^1.0.4",
